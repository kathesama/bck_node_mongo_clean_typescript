    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto-random-string": "^2.0.0",
    "ddos": "^0.2.1",
    "dotenv": "^10.0.0",
    "express": "^4.18.2",
    "express-fingerprint": "^1.2.2",
    "express-rate-limit": "^5.2.6",
    "express-validator": "^6.12.1",
    "fast-glob": "^3.2.5",
    "google-auth-library": "^7.14.1",
    "helmet": "^4.6.0",
    "http-status": "^1.5.0",
    "http-status-codes": "^2.1.4",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "micromatch": "^4.0.5",
    "moment": "^2.29.2",
    "moment-timezone": "^0.5.33",
    "mongoose": "^5.12.14",
    "mongoose-timezone": "^1.3.0",
    "multer": "^1.4.5-lts.1",
    "nocache": "^3.0.1",
    "react-google-login": "^5.2.2",
    "reflect-metadata": "^0.1.13",
    "supertest": "^6.1.3",
    "swagger-ui-express": "^4.1.6",
    "tsoa": "^3.8.0",
    "uuid": "^8.3.2",
    "validator": "^13.7.0",
    "winston": "^3.3.3"
  }
}
