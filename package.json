{
    "name": "granja_backend",
    "version": "1.0.0",
    "description": "Backend para el sistema de monitorizacion de granja",
    "main": "./src/main/server/server.ts",
    "scripts": {
        "build": "tsc",
        "start": "node ./dist/main/server/server.js",
        "start:dev": "cross-env NODE_ENV=development ts-node-dev --transpile-only --respawn --ignore-watch node_modules ./src/main/server/server.ts",
        "start:prod": "cross-env NODE_ENV=production ts-node-dev --transpile-only --respawn --ignore-watch node_modules ./src/main/server/server.ts",
        "test": "cross-env NODE_ENV=test jest --passWithNoTests --silent --noStackTrace --detectOpenHandles",
        "test:all": "cross-env NODE_ENV=test jest",
        "test:watch": "cross-env NODE_ENV=test jest -i --watch",
        "test:unit": "cross-env NODE_ENV=test npm test -- --watch -c ./jest-unit-config.ts",
        "test:integration": "cross-env NODE_ENV=test npm test -- --watch -c ./jest-integration-config.ts",
        "test:coverage": "cross-env NODE_ENV=test npm test -- --coverage",
        "test:clear": "jest --clearCache",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint . --fix",
        "prettier": "prettier --check **/*.ts",
        "prettier:fix": "prettier --write **/*.ts",
        "prepare": "husky install",
        "predev": "npm run swagger",
        "prebuild": "npm run swagger",
        "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec\"",
        "swagger": "tsoa spec"
    },
    "engines": {
        "node": ">=15.11.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kathesama/bck_node_mongo_clean.git"
    },
    "keywords": [
        "node",
        "mongoose",
        "cleancode"
    ],
    "author": "kathemica@gmail.com",
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/kathesama/bck_node_mongo_clean/issues"
    },
    "homepage": "https://github.com/kathesama/bck_node_mongo_clean#readme",
    "devDependencies": {
        "@babel/cli": "^7.14.5",
        "@babel/core": "^7.14.6",
        "@babel/eslint-parser": "^7.14.7",
        "@babel/eslint-plugin": "^7.14.5",
        "@babel/plugin-proposal-class-properties": "^7.14.5",
        "@babel/plugin-transform-runtime": "^7.14.5",
        "@babel/preset-env": "^7.14.7",
        "@babel/preset-typescript": "^7.14.5",
        "@babel/runtime": "^7.14.6",
        "@types/express": "^4.17.12",
        "@types/jest": "^26.0.24",
        "@types/lodash": "^4.14.170",
        "@types/mongoose": "^5.11.97",
        "@types/node": "^15.12.4",
        "@types/nodemailer": "^6.4.2",
        "@types/supertest": "^2.0.11",
        "@types/swagger-ui-express": "^4.1.2",
        "@types/tape": "^4.13.0",
        "@types/validator": "^13.1.4",
        "@typescript-eslint/eslint-plugin": "^4.27.0",
        "@typescript-eslint/parser": "^4.26.0",
        "concurrently": "^6.2.0",
        "cross-env": "^7.0.3",
        "eslint": "^7.28.0",
        "eslint-config-standard-with-typescript": "^20.0.0",
        "eslint-plugin-import": "^2.23.4",
        "eslint-plugin-jest": "^24.3.6",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.4.0",
        "eslint-plugin-promise": "^5.1.0",
        "eslint-plugin-standard": "^5.0.0",
        "git-commit-msg-linter": "^3.2.6",
        "husky": "^6.0.0",
        "istanbul": "^0.4.5",
        "jest": "^27.0.4",
        "lint-staged": "^11.0.0",
        "nodemon": "^2.0.7",
        "prettier": "^2.3.1",
        "ts-jest": "^27.0.4",
        "ts-node": "^10.1.0",
        "ts-node-dev": "^1.1.6",
        "typescript": "^4.3.2"
    },
    "dependencies": {
        "@sendgrid/mail": "^7.4.5",
        "axios": "^0.21.1",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.19.0",
        "cors": "^2.8.5",
        "crypto-random-string": "^2.0.0",
        "ddos": "^0.2.1",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "express-fingerprint": "^1.2.2",
        "express-rate-limit": "^5.2.6",
        "express-validator": "^6.12.1",
        "fast-glob": "^3.2.5",
        "helmet": "^4.6.0",
        "http-status": "^1.5.0",
        "http-status-codes": "^2.1.4",
        "joi": "^17.4.0",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.21",
        "moment": "^2.29.1",
        "moment-timezone": "^0.5.33",
        "mongoose": "^5.12.14",
        "mongoose-timezone": "^1.3.0",
        "reflect-metadata": "^0.1.13",
        "supertest": "^6.1.3",
        "swagger-ui-express": "^4.1.6",
        "tsoa": "^3.8.0",
        "validator": "^13.6.0",
        "winston": "^3.3.3"
    }
}
